<table class="table tablesorter">
  <thead>
  <tr>
    <th>Content</th>
    <th>Date</th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% notes.each do |note| %>
      <tr>
        <td><%= simple_format note.content %></td>
        <td><%= note.updated_at %></td>
        <td><%= actions_group edit_btn(edit_note_path(note)), delete_btn(note_path(note)) %></td>
      </tr>
  <% end %>
  </tbody>
</table>
