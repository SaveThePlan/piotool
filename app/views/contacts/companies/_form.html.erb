<%= simple_form_for(@contact) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :address %>
    <%= f.input :phone %>
    <%= f.input :website %>
    <%= f.input :siret %>
    <%= f.input :tva %>
    <%= f.input :employees_count %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
